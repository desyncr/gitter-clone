{
  "name": "gitter-clone",
  "version": "0.0.1",
  "description": "Gitter Clone",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./static/compiled/server.js",
    "dev": "nodemon src/server/server",
    "test": "jest",
    "lint": "eslint .",
    "build": "rimraf ./static/compiled && npm run build:node && npm run build:dist",
    "build:dist": "NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "build:node": "npm run babel -- ./src/server/ -d ./static/compiled",
    "babel": "babel --presets es2015,stage-0 --ignore node_modules,public"
  },
  "keywords": [
    "react",
    "redux",
    "express",
    "jest",
    "enzyme",
    "socket.io"
  ],
  "author": "Tyler Crosse",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongoose": "^4.7.7",
    "node-uuid": "^1.4.7",
    "normalizr": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-socket.io": "^1.3.1",
    "redux-thunk": "^2.1.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "clean-webpack-plugin": "^0.1.15",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.1.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "stats-webpack-plugin": "^0.4.3",
    "webpack": "^1.14.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0",
    "webpack-isomorphic-tools": "^2.6.6"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
